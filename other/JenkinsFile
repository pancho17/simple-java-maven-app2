pipeline{
    agent any

    enviroment{
        def myString = "Hello World"
        def myNumber = 10
        def myBool = true
    }

    stages{
        stage("Demo"){
            steps{
                echo "myString: ${myString}"
                echo "myNumber: ${myNumber}"
                echo "myBool: ${mybool}"
            }
        }
    }
}